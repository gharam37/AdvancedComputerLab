
public class State {

	String OneState;
	String ZeroState;
	String PrevState;
	String EpsilonState;
	String Name;
	
	public State(String Name,String Zero,String One) {
		this.Name=Name;
		this.ZeroState=Zero;
		this.OneState=One;
		
	}
	public State(String Name) {
		this.Name=Name;

	}
	public State(String Name,String Zero,String One,String epsilon) {
		this.Name=Name;
		this.ZeroState=Zero;
		this.OneState=One;
		this.EpsilonState=epsilon;
		
	}
	
	public String toString() {
		return this.Name+","+this.ZeroState+","+this.OneState+this.EpsilonState;
	}
}
