	    for(int i=0;i<rules.size();i++) {
	    	System.out.print(rules.get(i));
	    	Grammer.add(rules.get(i).split(","));
	    }
	    for(int i=0;i<Grammer.size();i++) {
	    	String []NewRule = Substitute(Grammer.get(i),Grammer,i);
	    }

	    
		return output;
	}
